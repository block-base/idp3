            }
          },
          {
            "path": ["$.vp.verifiableCredential[1].type"],
            "filter": {
              "type": "string",
              "pattern": "DelegateEthGTEOneTxnProvider"
            }
          }
        ]
      },
      "format": {
        "jwt_vp_json": {
          "proof_type": ["ES256K"]
        }
      }
    }
  ]
}
